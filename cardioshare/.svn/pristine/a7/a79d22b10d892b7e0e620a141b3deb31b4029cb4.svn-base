@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dbpedia: <http://dbpedia.org/ontology/> .
@prefix med: <http://sadiframework.org/ontologies/personalized-medicine/prescription.owl#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix lsrn: <http://purl.oclc.org/SADI/LSRN/> .
@prefix drugbank: <http://lsrn.org/DrugBank:> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

[]
  a foaf:Person;
  foaf:firstName "Jane";
  foaf:lastName "Doe";
  foaf:gender "female";
  dbpedia:birthDate "1975-03-23"^^xsd:date;
  med:prescription [
        a med:Prescription;
        sio:SIO_000628 drugbank:DB00404;           # SIO_000332 = 'refers to'
        med:dosage-form med:Pill;
        sio:SIO_000008 [                           # SIO_000008 = 'has attribute'
            a med:Dose;
            sio:SIO_000300 2;                      # SIO_000300 = 'has value'
            sio:SIO_000221 med:Milligrams          # SIO_000221 = 'has unit'
        ];
        sio:SIO_000008 [                           # SIO_000008 = 'has attribute'
            a sio:SIO_000052;                      # SIO_000052 = 'quantity'
            sio:SIO_000300 60                      # SIO_000300 = 'has value' (60 pills)
        ];
        sio:SIO_000008 [                           # SIO_000008 = 'has attribute'
            a sio:SIO_000031;                      # SIO_000031 = 'start date'
            sio:SIO_000300 "2012-01-03"^^xsd:date  # SIO_000300 = 'has value'
        ]
  ] .

drugbank:DB00404
  a med:PrescribedDrug;
  a sio:SIO_010004;                                # SIO_010038 = 'chemical entity'
  rdfs:label "Alprazolam";                         # anxiety drug
  sio:SIO_000008 [                                 # SIO_000008 = 'has attribute'
      a sio:SIO_000728;                            # SIO_000728 = 'chemical identifier'
      a lsrn:DrugBank_Identifier;
      sio:SIO_000300 "DB00404"                     # SIO_000300 = 'has value'
  ] .
