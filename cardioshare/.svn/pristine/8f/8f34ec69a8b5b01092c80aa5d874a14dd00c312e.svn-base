{
	predicate => 'http://es-01.chibi.ubc.ca/~benv/predicates.owl#isPaperAboutProtein',
	constructQueries => 
	[
		{
			outputType => [ 'http://biomoby.org/RESOURCES/MOBY-S/Objects/iHOPsymbol' ],
			query => 
			      "PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>  \n" .
			      "PREFIX moby: <http://mobyrdf/>   \n" .
			      "PREFIX mobyobj: <http://biomoby.org/RESOURCES/MOBY-S/Objects/>   \n" .
			      "PREFIX up: <http://uniprot/>    \n" .
			      "CONSTRUCT   \n" .
			      "{ \n" .
			      "    %%u%% up:isPaperAboutProtein ?crossref_node .  \n" .
			      "} \n" .
			      "WHERE  \n" .
			      "{  \n" .
			      "    ?crossref rdf:type mobyobj:CrossReference .  \n" .
			      "    ?crossref moby:hasObject ?crossref_node .  \n" .
			      "    ?crossref_node moby:hasNamespace 'UniProt' .  \n" .
			      "}",
		},
	],	

};