PREFIX sadi: <http://sadiframework.org/ontologies/properties.owl#>
PREFIX ss: <http://semanticscience.org/resource/>
PREFIX moby: <http://mobyrdf/> 
CONSTRUCT 
{    
     %u% %u% _:organism .
     _:organism sadi:hasName _:name .
     _:name ss:SIO_000300 ?organismStr . 
} 
WHERE 
{    
     ?root moby:hasArticleName "symbol" . 
     ?root moby:hasString ?organism .    
     ?organism moby:hasArticleName "organism" .    
     ?organism moby:hasContent ?organismStr .  
}
