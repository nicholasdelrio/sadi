PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX moby: <http://mobyrdf/> 
CONSTRUCT 
{    
     %u% %u% ?organismStr . 
} 
WHERE 
{    
     ?root moby:hasArticleName "symbol" . 
     ?root moby:hasString ?organism .    
     ?organism moby:hasArticleName "organism" .    
     ?organism moby:hasContent ?organismStr .  
}
