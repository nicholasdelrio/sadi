PREFIX ss: <http://semanticscience.org/resource/>
PREFIX moby: <http://mobyrdf/> 
CONSTRUCT 
{    
     %u% %u% _:description .
     _:description ss:SIO_000300 ?descStr . 
} 
WHERE 
{    
     ?root moby:hasArticleName "symbol" . 
     ?root moby:hasString ?desc .    
     ?desc moby:hasArticleName "description" .    
     ?desc moby:hasContent ?descStr . 
}
