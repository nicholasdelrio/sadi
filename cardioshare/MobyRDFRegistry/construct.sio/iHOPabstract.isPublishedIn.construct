PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX p: <http://sadiframework.org/ontologies/properties.owl#>
PREFIX ss: <http://semanticscience.org/resource/>
PREFIX moby: <http://mobyrdf/> 
CONSTRUCT 
{    
     %u% %u% _:journal .
     _:journal rdf:type ss:SIO_000160 .
     _:journal p:hasTitle ?journal .
} 
WHERE 
{    
     ?root moby:hasArticleName "magazine" . 
     ?root moby:hasContent ?journal .    
}
